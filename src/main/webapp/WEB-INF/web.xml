<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>l1</servlet-name>
    <servlet-class>com.app.LoginSrv</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>l1</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>lout</servlet-name>
    <servlet-class>com.app.LogOutSrv</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>lout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>sr1</servlet-name>
    <servlet-class>com.app.SubmitReimbursementSrv</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sr1</servlet-name>
    <url-pattern>/submit</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>vpr</servlet-name>
    <servlet-class>com.app.PendingReimbursementSrv</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>vpr</servlet-name>
    <url-pattern>/viewPendingReimbursement</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>vrr</servlet-name>
    <servlet-class>com.app.ResolvedReimbursementSrv</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>vrr</servlet-name>
    <url-pattern>/viewResolvedReimbursement</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>update</servlet-name>
    <servlet-class>com.app.UpdateProfileSrv</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>update</servlet-name>
    <url-pattern>/updateProfile</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>viewAll</servlet-name>
    <servlet-class>com.app.ViewAllReimbursementsSrv</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>viewAll</servlet-name>
    <url-pattern>/viewAllReimbursements</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>viewEmp</servlet-name>
    <servlet-class>com.app.ViewEmployeesSrv</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>viewEmp</servlet-name>
    <url-pattern>/viewEmployees</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>rev</servlet-name>
    <servlet-class>com.app.ReviewReimbursementsSrv</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>rev</servlet-name>
    <url-pattern>/review</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>regEmp</servlet-name>
    <servlet-class>com.app.RegisterEmployeeSrv</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>regEmp</servlet-name>
    <url-pattern>/regEmployee</url-pattern>
  </servlet-mapping>



</web-app>
